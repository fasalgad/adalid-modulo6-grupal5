 

<div class="container-fluid"  >
  <div class="row"> 
    {{#each pokemon}}
          <div class="col-lg-1 col-xl-1 col-md-2 col-3">
            <div class="card mb-4">
                <img id={{this.nombre}}  src="{{this.img}}" alt="{{this.nombre}}">
                <div class="card-text">
                  {{this.nombre}}
                </div>
            </div>
          </div>
    {{/each}}
  
  </div>
</div> 